<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <customSettingsVisibility>Public</customSettingsVisibility>
    <description>Settings that control whether webservice classes call out to find the latitude, longitude, federal congressional district, and state legislative district for contact and account addresses.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>GeocodeAccounts__c</fullName>
        <defaultValue>true</defaultValue>
        <externalId>false</externalId>
        <inlineHelpText>If checked, the trigger will try to look up latitude and longitude for accounts when they are created or when their address is changed.</inlineHelpText>
        <label>Geocode Accounts</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>GeocodeContacts__c</fullName>
        <defaultValue>true</defaultValue>
        <externalId>false</externalId>
        <inlineHelpText>If checked, the trigger will try to look up latitude and longitude for contacts when they are created or when their address is changed.</inlineHelpText>
        <label>Geocode Contacts</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>GeocoderPassword__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Password for paid account at geocoder.us webservice.</inlineHelpText>
        <label>Geocoder Password</label>
        <length>50</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>GeocoderUsername__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Username for paid account at geocoder.us webservice.</inlineHelpText>
        <label>Geocoder Username</label>
        <length>50</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>GetAccountDistricts__c</fullName>
        <defaultValue>true</defaultValue>
        <externalId>false</externalId>
        <inlineHelpText>If checked, the trigger will try to use the Mobile Commons webservice to look up congressional and state legislative districts for accounts when they are created or when address is changed.</inlineHelpText>
        <label>Get Account Districts</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>GetContactDistricts__c</fullName>
        <defaultValue>true</defaultValue>
        <externalId>false</externalId>
        <inlineHelpText>If checked, the trigger will try to use the Mobile Commons webservice to look up congressional and state legislative districts for contacts when they are created or when address is changed.</inlineHelpText>
        <label>Get Contact Districts</label>
        <type>Checkbox</type>
    </fields>
    <label>Geocoding Settings</label>
</CustomObject>
